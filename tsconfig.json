{
  "compilerOptions": {
    "paths": {
      "@/*": ["*"],
      "@libs/*": ["core/libs/*"],
      "@locales/*": ["core/locales/*"],
      "@sql/*": ["core/sql/*"],
      "@SQL": ["core/sql/index.ts"],
      "@typings": ["core/types/index.d.ts"],
      "@interfaces/*": ["core/types/*"],
      "@modules/*": ["core/modules/*"],
      "@bot": ["bot.ts"],
      "@config": ["config.ts"],
      "@context": ["context.ts"]
    },
    "baseUrl": "./src/",
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  },
  "typedocOptions": {
    "excludeExternals": true,
    "includeVersion": true,
    "media": "docs/assets/",
    "name": "CortanaTs",
    "out": "docs/",
    "readme": "README2.md"
  },
  "include": ["src/"]
}
