{
    "name": "CortanaTS",
    "description": "Powerfull telegram bot",
    "keywords": [
      "bot",
      "telegram bot",
      "typescript telegram bot"
    ],
    "website": "https://carlosburelo.tk/",
    "repository": "https://github.com/carlos-burelo/CortanaTS",
    "env": {
      "TOKEN": {
        "description": "TOKEN BOT FROM BOT FATHER",
        "required": true
      },
      "NODE_ENV": {
        "description": "TOKEN BOT FROM BOT FATHER",
        "value":"prod",
        "required": true
      },
      "URL": {
        "description": "your host url from heroku",
        "value":"https://<your app name>.herokuapp.com",
        "required": true
      },
      "CURRENCY": {
        "description": "Currency api key",
        "required":true
      },
      "CHANELID":{
        "description": "Your chanel for receive logs",
        "required":true
      },
      "OWNERID":{
        "description": "Your own id",
        "required":true
      },
      "OWNERUSERNAME":{
        "description": "Your @username",
        "required":true
      },
      "OWNERNAME":{
        "description": "Your name",
        "required":true
      },
      "BOTID":{
        "description": "Your bot id",
        "required":true
      },
      "BOTUSERNAME":{
        "description": "Your bot @username",
        "required":true
      },
      "BOTNAME":{
        "description": "Your bot username",
        "required":true
      },
      "BOTREPO":{
        "description": "Bot repository",
        "required":true
      }
    }
  }