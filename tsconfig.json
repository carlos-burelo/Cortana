{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": "src",
    "skipLibCheck": true,
    "strict": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
